<%- include("partials/header")-%>
    <h1>Compose</h1>

    <form action="/compose" method="post">
        <div class="form-group">
            <label>Title</label>
            <input type="text" name="postTitle" required autocomplete="off" class="form-control">
            <br>
            <label>Post</label>
            <textarea name="postBody" class="form-control" cols="50" rows="10"></textarea>
            <br>
            <button class="btn btn-primary" type="submit" name="button">Publish</button>
        </div>
    </form>



    <%- include("partials/footer")-%>